   "source": [
    "from gtts import gTTS\n",
    "import os\n",
    "text = \"Hello, how are you doing today?\"\n",
    "language = 'en'\n",
    "speech = gTTS(text=text, lang=language, slow=False)\n",
    "speech.save(\"hello.mp3\")\n",
    "os.system(\"mpg321 hello.mp3\")"
   ]
